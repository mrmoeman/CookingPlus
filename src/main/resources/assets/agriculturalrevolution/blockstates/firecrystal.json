{
    "multipart": [
        {   "when": { "down": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj" }
        },
        {   "when": { "up": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj", "x": 180, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj", "x": 90, "uvlock": true }
        },
        {   "when": { "north": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj", "x": 270, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj", "x": 90, "y": 90, "uvlock": true }
        },
        {   "when": { "east": true },
            "apply": { "model": "agriculturalrevolution:firecrystalgrowth.obj", "x": 90, "y": 270, "uvlock": true }
        }
    ]
}
